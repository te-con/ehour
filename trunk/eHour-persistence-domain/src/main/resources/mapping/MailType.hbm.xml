<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="net.rrm.ehour.domain.MailType" table="MAIL_TYPE"  mutable="false">
	    <cache usage="read-only" />
		
		<id name="mailTypeId" column="MAIL_TYPE_ID" type="integer" />

		<property name="mailType" column="MAIL_TYPE" type="string" length="64" index="IDX_MAILTYPE" />
	</class>
</hibernate-mapping>