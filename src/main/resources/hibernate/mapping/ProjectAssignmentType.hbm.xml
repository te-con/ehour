<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="net.rrm.ehour.project.domain.ProjectAssignmentType" table="PROJECT_ASSIGNMENT_TYPE">
	    <cache usage="read-only" />
		
		<id name="assignmentTypeId" column="ASSIGNMENT_TYPE_ID" type="integer">
			<generator class="assigned" />
       	</id>

        <property name="assignmentType" column="ASSIGNMENT_TYPE" type="string" />
	</class>	
</hibernate-mapping>
