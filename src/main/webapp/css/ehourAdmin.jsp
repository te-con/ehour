<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<%@ page contentType="text/css;" %>

a:link
{
	color: #233e55;
	font-weight: normal;
	text-decoration: none;
}

a:visited
{
	color: #233e55;
	font-weight: normal;	
	text-decoration: none;
}

a:hover
{
	text-decoration: underline;
}

a:active
{
	text-decoration: none
}

.contentTable
{
	padding: 0px;
	margin: 0 0 0 10px;
	spacing: 0px;
}

.contentTable TR
{
	border: 0px;
	padding: 0px;
	margin: 0px;
}

.contentTable TH
{
	padding: 0px;
	margin: 0px;
	font:  bold 0.9em/1em Geneva, Arial, Helvetica, sans-serif;
	text-align: left;
	color: #233e55;
}

.contentTable TD
{
	padding: 0px;
	margin: 0px;
	font:  1em Verdana Geneva, Arial, Helvetica, sans-serif;
	border: 0px;
	text-align: left;
	color: #6c87a1;
}

.contentTable td.main
{
	color: #233e55;
}

.contentTable td.extra
{
	padding: 0 0 0 5px;
	margin: 0px;
	font:  1em Verdana Geneva, Arial, Helvetica, sans-serif;
	border: 0px;
	text-align: left;
	color: #86a1bc;
	border-bottom: 1px solid #6c87a1;
}

.errorCell
{
	border: 0px;
	padding: 0px;
	margin: 0px;
	font-family: 8pt Verdana, Geneva, Arial, Helvetica;
	color: #ff00ff;
}

div.adminListScroll
{
	height:	200px;
	margin: 0 5px 0 5px;
	overflow: auto;
	padding: 0px;
	width: 210px;
	font:  1em Geneva, Arial, Helvetica, sans-serif;
}

div.GreyNavBody li a:link
{
	text-decoration: none;
	font: 1em Verdana, Geneva, Arial, Helvetica;
}

div.GreyNavBody li a:visited
{
	text-decoration: none;
	font: 1em Verdana, Geneva, Arial, Helvetica;
}

div.GreyNavBody li a:hover
{
	text-decoration: underline;
	font: 1em Verdana, Geneva, Arial, Helvetica;
	color: #233e55;
}

div.GreyNavBody li a:active
{
	text-decoration: underline;
	font: 1em Verdana, Geneva, Arial, Helvetica;
}