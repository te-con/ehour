<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="wicketApplication" class="net.rrm.ehour.ui.EhourWebApplication">
		<property name="authenticationManager" ref="authenticationManager" />
		<property name="version" value="0.8.1"></property>
		<property name="pageConfig" ref="pageConfig" />
		<property name="authorizationStrategyFactory" ref="authorizationStrategyFactory"/>
	</bean>
	
	<bean id="pageConfig" class="net.rrm.ehour.ui.config.PageConfigImpl" />
</beans>
